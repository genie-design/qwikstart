{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.21.0",
    "@builder.io/qwik-city": "0.5.3",
    "@iconify-json/fe": "^1.1.6",
    "@julr/unocss-preset-forms": "^0.0.3",
    "@types/eslint": "8.21.1",
    "@types/node": "^18.15.0",
    "@types/node-fetch": "latest",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@unocss/preset-attributify": "^0.50.4",
    "@unocss/preset-icons": "^0.50.4",
    "@unocss/preset-web-fonts": "^0.50.4",
    "@unocss/preset-wind": "^0.50.4",
    "@unocss/reset": "^0.50.4",
    "@unocss/transformer-directives": "^0.50.4",
    "@vanilla-extract/css": "^1.10.0",
    "eslint": "8.36.0",
    "eslint-plugin-qwik": "0.21.0",
    "node-fetch": "3.3.1",
    "prettier": "2.8.4",
    "styled-vanilla-extract": "^0.5.4",
    "typescript": "4.9.5",
    "undici": "5.20.0",
    "unocss": "^0.50.4",
    "unocss-preset-theme": "^0.5.0",
    "vite": "4.1.4",
    "vite-tsconfig-paths": "4.0.5"
  },
  "dependencies": {
    "i": "^0.3.7"
  }
}
